# TAL Deployment Configuration
# Copy this file to .env and fill in your values
# NEVER commit .env to version control!

# Required: Deployer private key (without 0x prefix)
PRIVATE_KEY=

# Required: RPC endpoint for deployment
RPC_URL=http://localhost:8545

# Optional: External contract addresses (leave empty if not available)
STAKING_V2=
ZK_VERIFIER=
DRB_COORDINATOR=
TEE_ORACLE=

# Optional: Etherscan API key for verification
ETHERSCAN_API_KEY=
