{
  "name": "@tokamak/execution-kernel-sdk",
  "version": "0.1.0",
  "description": "TypeScript SDK for Tokamak Execution Kernel - verifiable agent execution with RISC Zero proofs",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "viem": "^2.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "tsup": "^8.3.0",
    "typescript": "^5.6.0",
    "vitest": "^2.1.0"
  },
  "peerDependencies": {
    "viem": ">=2.0.0"
  },
  "files": ["dist"],
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/tokamak-network/Tokamak-AI-Layer"
  },
  "keywords": ["tokamak", "execution-kernel", "risc-zero", "zk", "agent", "vault", "sdk"]
}
